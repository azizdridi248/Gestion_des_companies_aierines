<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.WebView?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.WebController">
    <children>
        <WebView fx:id="webView" layoutX="10" layoutY="10">
            <AnchorPane.topAnchor>0.0</AnchorPane.topAnchor>
            <AnchorPane.bottomAnchor>50.0</AnchorPane.bottomAnchor>
            <AnchorPane.leftAnchor>0.0</AnchorPane.leftAnchor>
            <AnchorPane.rightAnchor>0.0</AnchorPane.rightAnchor>
        </WebView>
        <TextField fx:id="TextField" layoutX="10" layoutY="420" prefHeight="30">
            <AnchorPane.bottomAnchor>10.0</AnchorPane.bottomAnchor>
            <AnchorPane.leftAnchor>10.0</AnchorPane.leftAnchor>
            <AnchorPane.rightAnchor>100.0</AnchorPane.rightAnchor>
        </TextField>
        <Button fx:id="LoadButton" layoutX="420" layoutY="420" text="Load" onAction="#loadPage">
            <AnchorPane.bottomAnchor>10.0</AnchorPane.bottomAnchor>
            <AnchorPane.rightAnchor>10.0</AnchorPane.rightAnchor>
        </Button>
    </children>
</AnchorPane>
